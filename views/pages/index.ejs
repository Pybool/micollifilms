<!DOCTYPE html>
<html class="no-js" lang="zxx">
<%- include('../partials/head'); %>

  <style>
    .no-display {
      display: none;
    }

    .form-control {
      background: transparent !important;
    }

    .image-background {
      margin-top: -20px;
      min-height: 100vh;
      background-image: url(/assets/img/micolli/compressed/DAP_6317.png);
      background-repeat: no-repeat;
      background-position: center center;
      background-size: cover !important;
      transition: background-image 3s ease-in-out;
    }

    .wp-image {
      position: absolute;
      top: 10px;
      right: 10px;
      color: white;
      gap: 20px;
      background: black;
      padding: 10px;
      z-index: 11;

    }

    .wp-image>a {
      font-size: small !important;
    }

    .change-bg {
      background: white !important;
      color: black !important;
      z-index: 1;
    }

    .flex-row-start {
      display: flex;
      justify-content: start;
      align-items: center;
      gap: 10px;
    }

    .main-h2 {
      font-size: 5.5rem !important;
      text-align: center;
      position: relative;
      z-index: 1;
    }

    /* For mobile devices */
    @media (max-width: 767px) {
      .image-background {
        background-size: cover !important;
      }
    }

    .banner-area {
      position: relative;
      /* Ensure overlay and content are positioned relative to the section */
    }

    .overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.6);
      /* Dark overlay with transparency */
      z-index: 0;
      /* Put overlay behind other content */
    }
  </style>
  <style>
    .astonish {
      visibility: visible;
    }

    @media (min-width: 768px) {
      .astonish {
        visibility: hidden;
      }

      .animated {
        visibility: visible;
      }
    }


    @media only screen and (max-width:600px) {
      .main-h2 {
        font-size: 2.5rem !important
      }

      h3 {
        font-size: 1.5rem !important
      }

      .img-name {
        font-size: 0.8rem !important
      }

      .active {
        margin-top: 100px !important;
      }

      .arrow {
        display: none !important;
      }


    }
  </style>
